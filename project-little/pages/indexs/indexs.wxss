.floor_list{overflow: hidden;}

.floor_list>navigator{
  float: left;
  width: 33.33%;
  
}
.floor_list>navigator:nth-last-child(-n+4){
  /*以首图宽高为标准，假设为宽高232*386----------公式是386/232/2*33.33vw*/
  height:27.7vw;
  border-left: 10rpx solid #fff;
}
/*单选23两张图*/
.floor_list>navigator:nth-last-child(3){border-bottom: 10rpx solid #fff;}
.floor_list>navigator:nth-last-child(4){border-bottom: 10rpx solid #fff;}
.floor_list>navigator>image{
  width:100%;
  height:100%;
}